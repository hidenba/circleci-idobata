%p
  #{json['username']}/#{json['reponame']}#
  %a{href: json['build_url']}= json['build_num']
  (
  #{json['branch']} #{json['vcs_revision'][0..5]}:
  )
  - if json['status'] == 'success'
    %span.label.label-success= json['status']
  -else
    %span.label.label-important= json['status']
  (#{json['build_time_millis']}msec)
%p
  #{json['committer_name']}: #{json['subject']}
  (
  %a{href: "#{json['vcs_url']}/#{json['vcs_revision']}"} commit
  )
